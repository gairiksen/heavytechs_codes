{% liquid
  if uuid == blank and slug == blank and id == blank and user_id == blank and anonymous_session_id == blank
    return null
  endif
%}

{% graphql r = 'mechanic/search_params', limit: 1, user_id: user_id %}

{% liquid
  include 'lib/handle_graphql_response', r: r, name: 'queries/profiles/find_mechanic_search_params'

  return r.records.results.first
%}
