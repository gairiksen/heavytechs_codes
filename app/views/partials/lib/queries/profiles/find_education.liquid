{% liquid
  if uuid == blank and slug == blank and id == blank and user_id == blank and anonymous_session_id == blank
    return null
  endif
%}

{% graphql r = 'profiles/search_education', limit: 1, id: id, user_id: user_id %}

{% liquid
  include 'lib/handle_graphql_response', r: r, name: 'queries/profiles/find_education'

  return r.records.results.first
%}
