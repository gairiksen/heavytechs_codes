            assign crane_certification = '' | default: ''
            assign crane_cert_expiry_date = '' | default: ''
            assign capacity = '' | default: ''
        endif

        if truck_type == 'Lube Truck'
            assign no_of_products_tank = object.no_of_products_tank | default: ''
            assign fuel_tank_size = object.fuel_tank_size | default: ''
        else 
            assign no_of_products_tank = '' | default: ''
            assign fuel_tank_size = '' | default: ''
        endif
        
        assign truck_class = object.truck_class | default: ''
        assign air_compressor = object.air_compressor | default: ''
