{%liquid

    assign is_truck = object.is_truck | default: ''

    if is_truck == 'Yes'

        assign short_description_service_skills = object.short_description_service_skills | default: ''
        assign truck_type = object.truck_type | default: ''

        if truck_type == 'Service Truck (with crane)'
            assign crane_certification = object.crane_certification | default: ''
            assign crane_cert_expiry_date = object.crane_cert_expiry_date | default: ''
            assign capacity = object.capacity | default: ''
