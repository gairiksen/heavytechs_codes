{% parse_json object %}
  {
    "uuid":         {{ '' | uuid | json }},
    "user_id":      {{ object.user_id | json }},
    "first_name":   {{ object.first_name | json }},
    "last_name":    {{ object.last_name | json }},
    "language":    {{ object.language | json }},
    "profile_for":    {{ object.profile_for | json }}
  }
{% endparse_json %}

{% liquid
  return object
%}
