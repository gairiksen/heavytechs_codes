{% parse_json object %}
  {
    "uuid":   {{ '' | uuid | json }},
    "direct_url":  {{ object.direct_url | json }},
    "user_id":     {{ context.current_user.id | json }},
    "file_type":     {{ object.photo_type | json }},
    "item_category":   {{ object.item_category | json }},
    "page_category":   {{ object.page_category | json }},
    "is_active":   {{ '0' | json }}
  }
{% endparse_json %}

{% liquid
  return object
%}
