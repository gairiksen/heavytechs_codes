<div class="profileEdit bg-gray-100 pt-9 pb-28"><!--profileEdit-->
<div class="container m-auto px-3.5">

<div class="flex justify-between flex-col md:flex-row">
<div class="w-full mb-6 md:mb-0 md:w-60">

    
    {% render 'theme/simple/dashboard/profile/contractor/photo_upload_area', object: object, presigned_url: presigned_url %}
    {% render 'theme/simple/dashboard/profile/contractor/profile_completion_bar', object: object, search_mechanic_deatils: search_mechanic_deatils %}


  <div class="pt-8 pb-5 md:pb-0">
      <ul class="customRadio flex md:block">
          <li class="mr-4 md:mr-0 md:mb-3">
              <input {%if profile_availability.availability == '1'%}checked{%endif%} id="available" class="mechanic_availability" type="radio" name="available" value="1">
              <label for="available" class="text-base font-medium text-gray-800"><span><span></span></span>Available</label>
          </li>
          <li>
              <input {%if profile_availability.availability == '0'%}checked{%endif%} id="unavailable" class="mechanic_availability" type="radio" name="available" value="0">
              <label for="unavailable" class="text-base font-medium text-gray-800"><span><span></span></span>Unavailable</label>
          </li>
      </ul>
  </div>

</div>

<div class="profileEdit_right w-full" x-data="{ 
                    openTab: 1, 
                    activeClasses:'font-medium heavytechs-text-dark-orange border-b-2 heavytechs-border-color-orange',
                    inactiveClasses:'text-gray-800 font-normal'
                }">
    <div class="mb-8 bg-white">
        <ul class="flex items-center flex-col md:flex-row">
            <li class="w-full md:w-auto" @click="openTab = 1">
                <a href="javascript:void(0)" :class="openTab === 1 ? activeClasses : inactiveClasses" class="text-base font-normal text-gray-800 w-full text-center h-12 flex items-center justify-center px-3">Bio</a>
            </li>
            <li class="w-full md:w-auto" @click="openTab = 2">
                <a href="javascript:void(0)" :class="openTab === 2 ? activeClasses : inactiveClasses" class="text-base font-normal text-gray-800 w-full text-center h-12 flex items-center justify-center px-3">Skills/Services</a>
            </li>
            <li class="w-full md:w-auto" @click="openTab = 3">
                <a href="javascript:void(0)" :class="openTab === 3 ? activeClasses : inactiveClasses" class="text-base font-normal text-gray-800 w-full text-center h-12 flex items-center justify-center px-3">Education/Certs</a>
            </li>
            <li class="w-full md:w-auto" @click="openTab = 4">
                <a href="javascript:void(0)" :class="openTab === 4 ? activeClasses : inactiveClasses" class="text-base font-normal text-gray-800 w-full text-center h-12 flex items-center justify-center px-3">Truck</a>
            </li>
        </ul>
    </div>

    <div class="bg-white rounded-md" x-show="openTab === 1">

        <div>
        <div class="bg-white rounded-md shadow-sm px-6 pt-8 pb-1 relative">
            <form action="#">
                <div class="absolute right-7 top-10">
                    <a href="/dashboard/profile/bio?edit=1">
                        <img src="{{ 'images/edit-icon.svg' | asset_url }}" alt="">
                    </a>
                </div>

                <div class="mb-8">
                    <h4 class="block text-base font-bold text-gray-600 mb-2">Company Name</h4>
                    <span class="block text-base text-gray-500">{{profile_bio.company_name}}</span>
                </div>

                <div class="mb-8">
                    <h4 class="block text-base font-bold text-gray-600 mb-2">Position</h4>
                    <p class="block text-base text-gray-500">{{profile_bio.position}}</p>
                </div>

                <div class="mb-8">
                    <h4 class="block text-base font-bold text-gray-600 mb-2">Location:</h4>
                    <span class="block text-base text-gray-500">{{profile_bio.location}}</span>
                </div>

                <div class="mb-8">
                    <h4 class="block text-base font-bold text-gray-600 mb-2">Description:</h4>
                    <span class="block text-base text-gray-500">{{profile_bio.long_description}}</span>
                </div>

                <div class="mb-8">
                    <h4 class="block text-base font-bold text-gray-600 mb-2">How far you prepeared to travel:</h4>
                    <span class="block text-base text-gray-500">{{profile_bio.ready_for_travel}}</span>
                </div>

                <div class="mb-8">
                    <h4 class="block text-base font-bold text-gray-600 mb-2">Experience:</h4>
                    <span class="block text-base text-gray-500">{{profile_bio.total_experience}}</span>                    
                </div>

                <div class="mb-8">
                    <h4 class="block text-base font-bold text-gray-600 mb-2">Veteran:</h4>
                    <span class="block text-base text-gray-500">{{profile_bio.veteran}}</span>                    
                </div>

            </form>
        </div>
        </div>

    </div>

    <div x-show="openTab === 2" style="display: none;">
        <div>
        <div class="bg-white rounded-md">
        <form>
            <div class="bg-white rounded-md shadow-sm px-6 pt-8 pb-1 relative">
                <div class="absolute right-7 top-10">
                    <a href="/dashboard/profile/skills">
                        <img src="{{ 'images/edit-icon.svg' | asset_url }}" alt="">
                    </a>
                </div>

                <div class="mb-11">
                    <h4 class="text-base font-normal text-gray-800 mb-3.5">Industries Serviced (project type)</h4>

                    <ul class="customCheck gap-4 flex flex-col flex-wrap w-full md:w-2/4 md:flex-row md:gap-y-2">
                        {%if profile_skills.earthworks == '1' %}
                        <li>
                            <input id="projecttype2" type="checkbox" checked disabled>
                            <label for="projecttype2" class="text-base font-medium text-gray-800">
                            <span><span></span></span>Earthworks</label>
                        </li>
                        {%endif%}

                        {%if profile_skills.mining == '1' %}
                        <li>
                            <input id="projecttype2" type="checkbox" checked disabled>
                            <label for="projecttype2" class="text-base font-medium text-gray-800">
                            <span><span></span></span>Mining</label>
                        </li>
                        {%endif%}

                        {%if profile_skills.fleet == '1' %}
                        <li>
                            <input id="projecttype2" type="checkbox" checked disabled>
                            <label for="projecttype2" class="text-base font-medium text-gray-800">
                            <span><span></span></span>Fleet</label>
                        </li>
                        {%endif%}

                        {%if profile_skills.pipeline == '1' %}
                        <li>
                            <input id="projecttype2" type="checkbox" checked disabled>
                            <label for="projecttype2" class="text-base font-medium text-gray-800">
                            <span><span></span></span>Pipeline</label>
                        </li>
                        {%endif%}

                        {%if profile_skills.energy_production == '1' %}
                        <li>
                            <input id="projecttype2" type="checkbox" checked disabled>
                            <label for="projecttype2" class="text-base font-medium text-gray-800">
                            <span><span></span></span>Energy Production</label>
                        </li>
                        {%endif%}
                    </ul>
                </div>

                <div class="w-full mb-5 md:w-4/6 md:mb-16">
                    <h4 class="text-base font-normal text-gray-800 mb-3.5">Skills/ Service Offering</h4>
                    <ul class="customCheck flex flex-col flex-wrap gap-4 md:flex-row md:gap-y-2">
                        {%if profile_skills.heavy_equipment_mechanic == '1' %}
                        <li>
                            <input id="skills2" type="checkbox" checked disabled>
                            <label for="skills2" class="text-base font-medium text-gray-800">
                            <span><span></span></span>Heavy Equipment Mechanic</label>
                        </li>
                        {%endif%}

                        {%if profile_skills.welder_fabricator == '1' %}
                        <li>
                            <input id="skills2" type="checkbox" checked disabled>
                            <label for="skills2" class="text-base font-medium text-gray-800">
                            <span><span></span></span>Welder/Fabricator</label>
                        </li>
                        {%endif%}

                        {%if profile_skills.truck_mechanic == '1' %}
                        <li>
                            <input id="skills2" type="checkbox" checked disabled>
                            <label for="skills2" class="text-base font-medium text-gray-800">
                            <span><span></span></span>Truck Mechanic</label>
                        </li>
                        {%endif%}

                        {%if profile_skills.oilers_pm_tech == '1' %}
                        <li>
                            <input id="skills2" type="checkbox" checked disabled>
                            <label for="skills2" class="text-base font-medium text-gray-800">
                            <span><span></span></span>Oilers/PM Tech</label>
                        </li>
                        {%endif%}
                    </ul>
                </div>


                <div class="bg-white-50 py-5 px-7 -mx-6 mt-11">
                    <h4 class="text-base font-normal text-gray-800 mb-2.5">OEM</h4>
                    <ul class="customCheck flex items-center flex-col gap-4 md:flex-row md:gap-x-10 flex-wrap">
                        {%
                        for oem in profile_all_oem
                        %}
                        <li>
                            <input id="OEM2" type="checkbox" checked disabled>
                            <label for="OEM2" class="text-base font-medium text-gray-800">
                                <span><span></span></span>
                                {{oem.oem_name}}<br>
                                ({{ oem.expert_familiar | capitalize }})

                                {% for up in user_pdf.results %}
                                    {% if up.item_category == oem.oem_name %}
                                        <a href="{{up.direct_url}}">View Certificate</a>
                                    {% endif %}
                                {% endfor %}
                            </label>
                        </li>
                        {% endfor %}
                    </ul>
                </div>


                <div class="py-10">
                    <h4 class="text-base font-normal text-gray-800 mb-5">Drivers License</h4>
                    <ul class="customCheck flex items-center flex-col gap-4 mb-9 md:flex-row md:gap-x-14">
                        <li>
                            <input id="Licence1" type="checkbox" checked="" disabled>
                            <label for="Licence1" class="text-base font-medium text-gray-800">
                            <span><span></span></span>{{profile_skills.drivers_license_class}}</label>
                        </li>                    
                    </ul>
                </div>

                <div class="mb-10">
                    <ul class="customCheck flex flex-col gap-4">
                        {% if profile_skills.tanker == '1' %}
                        <li>
                            <input id="Tanker1" type="checkbox" checked disabled>
                            <label for="Tanker1" class="text-base font-medium text-gray-800">
                            <span><span></span></span>Tanker</label>
                        </li>
                        {% endif %}
                        
                        {% if profile_skills.air_brakes == '1' %}
                        <li>
                            <input id="Tanker1" type="checkbox" checked disabled>
                            <label for="Tanker1" class="text-base font-medium text-gray-800">
                            <span><span></span></span>Air Brakes</label>
                        </li>
                        {% endif %}
                        
                        {% if profile_skills.passenger == '1' %}
                        <li>
                            <input id="Tanker1" type="checkbox" checked disabled>
                            <label for="Tanker1" class="text-base font-medium text-gray-800">
                            <span><span></span></span>Passenger</label>
                        </li>
                        {% endif %}

                        {% if profile_skills.doubles_triples == '1' %}
                        <li>
                            <input id="Tanker1" type="checkbox" checked disabled>
                            <label for="Tanker1" class="text-base font-medium text-gray-800">
                            <span><span></span></span>Doubles/Triples</label>
                        </li>
                        {% endif %}
                        
                        {% if profile_skills.hazmat == '1' %}
                        <li>
                            <input id="Tanker1" type="checkbox" checked disabled>
                            <label for="Tanker1" class="text-base font-medium text-gray-800">
                            <span><span></span></span>Hazmat</label>
                        </li>
                        {% endif %}                   
                    </ul>
                </div>

                <div class="w-full mb-5 md:w-4/6 md:mb-16">
                    <span class="block text-base text-gray-500 mb-4">Expiration date of hazmat endorsement: 
                    <strong>{{ profile_skills.expiry_date_2 | date: "%d.%m.%Y" }}</strong></span>
                </div>

                <div>
                    <h4 class="mb-3.5 text-base text-gray-700 font-bold">Insurance Coverage</h4>
                    <span class="block text-base text-gray-500 mb-2">Expiration date: 
                    <strong>{{ profile_insurance.expiration_date | date: "%d.%m.%Y" }}</strong></span>
                    <ul>
                        <li class="flex items-center mb-3">
                            <span class="block text-base text-gray-600 w-1/4">
                                Insurance
                            </span>
                            {% for up in user_pdf.results %}
                                {% if up.page_category == 'skills' and up.item_category == 'insurance' %}
                                    <a href="{{up.direct_url}}" class="text-base text-blue-500 hover:underline">View Certificate</a>
                                {% endif %}
                            {% endfor %}
                        </li>
                        
                    </ul>
                </div>

            </div>

        </form>
        </div>
        </div>

    </div>
    <div x-show="openTab === 3" style="display: none;">
        <div>
        <div class="bg-white rounded-md">
        <form action="#">            
            <div class="bg-white rounded-md shadow-sm px-6 pt-8 pb-1 relative">
                <div class="absolute right-7 top-10">
                    <a href="/dashboard/profile/education">
                        <img src="{{ 'images/edit-icon.svg' | asset_url }}" alt="">
                    </a>
                </div>

                <div class="bg-white">
                    <div class="customCheck pb-5">                
                        <label class="text-base font-medium text-gray-800" for="automotive1">
                        <span></span>Automotive Service Excellence (ASE) certifications</label>                  
                    </div>

                    {% if profile_education.automotive_service_excellence == '1' %}
                    <ul class="mb-11">
                        {% for ase in profile_ase %}
                        <li class="flex items-center mb-3">
                            <span class="block text-base text-gray-600">
                                {{ase.ase_name}}
                            </span>
                            {% for up in user_pdf.results %}
                                {% if up.page_category == 'education' and  up.item_category == ase.ase_name %}
                                    <a href="{{up.direct_url}}" class="text-base text-blue-500 hover:underline">View Certificate</a>
                                {% endif %}
                            {% endfor %}                    
                        </li>
                        {% endfor %}
                    </ul>
                    {% endif %}
                </div>

                <div class="bg-white">
        
                    <div class="customCheck pb-5">
                        <label for="OSHA1" class="text-base font-medium text-gray-800">
                        <span></span>OSHA (Occupation Safety Hazard Association)</label>                        
                    </div>
                    <span class="block text-base text-gray-500 mb-2">Expiration date: 
                    <strong>{{ profile_education.osha_expiry | date: "%d.%m.%Y" }}</strong></span>


                    {% if profile_education.osha == '1' %}
                    <ul class="mb-11">
                        <li class="flex items-center mb-3">
                            <span class="block text-base text-gray-600 w-2/4">
                                OSHA (Occupation Safety Hazard Association)
                            </span>
                            {% for up in user_pdf.results %}
                                {% if up.page_category == 'education' and  up.item_category == 'osha_certificate' %}
                                    <a href="{{up.direct_url}}" class="text-base text-blue-500 hover:underline">View Certificate</a>
                                {% endif %}
                            {% endfor %}
                        </li>
                    </ul>
                    {% endif %}
                </div>

                <div class="bg-white">
        
                    <div class="customCheck pb-5">
                        <label for="OSHA1" class="text-base font-medium text-gray-800">
                        <span></span>MSHA (Mining Safety Hazard Association)</label>                        
                    </div>
                    <span class="block text-base text-gray-500 mb-2">Expiration date: 
                    <strong>{{ profile_education.msha_expiry | date: "%d.%m.%Y" }}</strong></span>


                    {% if profile_education.osha == '1' %}
                    <ul class="mb-11">
                        <li class="flex items-center mb-3">
                            <span class="block text-base text-gray-600 w-2/4">
                                MSHA (Mining Safety Hazard Association)
                            </span>
                            {% for up in user_pdf.results %}
                                {% if up.page_category == 'education' and  up.item_category == 'msha_certificate' %}
                                    <a href="{{up.direct_url}}" class="text-base text-blue-500 hover:underline">View Certificate</a>
                                {% endif %}
                            {% endfor %}
                        </li>
                    </ul>
                    {% endif %}
                </div>


                <div class="mb-11">
                    <div class="customCheck pb-7">
                        <label for="military1" class="text-base font-medium text-gray-800">
                        <span></span>Military Training</label>                        
                    </div>

                    {% if profile_education.military_training == '1' %}
                    <span class="block text-base text-gray-500 mb-2.5">{{profile_education.most_advanced_training}}</span>
                    <span class="block text-base text-gray-500 mb-4">Date completed: <strong>{{ profile_education.date_of_completion | date: "%d.%m.%Y" }}</strong></span>
                    <p class="block text-base text-gray-500">{{profile_education.other_military_training}}</p>
                    {% endif %}
                </div>

                <div>
                    <div class="customCheck pb-7">
                        <label for="College1" class="text-base font-medium text-gray-800">
                        <span></span>College degree</label>                        
                    </div>

                    {% if profile_education.college_degree == '1' %}
                    <span class="block text-base text-gray-500 mb-2.5">{{profile_education.highest_degree}}</span>
                    <span class="block text-base text-gray-500 mb-4">Date completed: <strong>{{ profile_education.highest_degree_d_o_completion | date: "%d.%m.%Y" }}</strong></span>
                    <div class="flex items-center">
                        <p class="block text-base text-gray-600 w-2/4">
                            Diploma
                        </p>
                        {% for up in user_pdf.results %}
                            {% if up.page_category == 'education' and  up.item_category == 'highest_degree' %}
                                <a href="{{up.direct_url}}" class="text-base text-blue-500 hover:underline">View Certificate</a>
                            {% endif %}
                        {% endfor %}
                        <!--<a href="#" class="text-base text-blue-500 hover:underline">View diploma</a>-->
                    </div>
                    {% endif %}
                </div>

            </div>
        </form>
        </div>
        </div>
    </div>
    <div x-show="openTab === 4" style="display: none;">
        <div>
        <div class="bg-white rounded-md">
        <form action="#">
        <div class="bg-white rounded-md shadow-sm px-6 pt-8 pb-1 relative">
            <div class="absolute right-7 top-10">
                <a href="/dashboard/profile/truck">
                    <img src="{{ 'images/edit-icon.svg' | asset_url }}" alt="">
                </a>
            </div>
            <div class="mb-9">
                <h4 class="mb-5 text-base text-gray-700 font-bold">{{mechanic_basic_details.first_name}}â€™s Truck</h4>
                <p class="block text-base text-gray-500">{{profile_truck.short_description_service_skills}}</p>
            </div>

            <div class="mb-11">
                <ul class="flex flex-col gap-4 md:flex-row">
                    {% for tImg in truck_photos %}
                    <li>
                        <figure><img style="width: 175px; height: 95px;" src="{{tImg.photo_path}}" alt="" class="rounded"></figure>
                    </li>
                    {%endfor%}
                    <!--<li>
                        <figure>
                            <img src="images/truck-img.jpg" alt="" class="rounded">
                        </figure>
                    </li>
                    <li>
                        <figure>
                            <img src="images/truck-img.jpg" alt="" class="rounded">
                        </figure>
                    </li>
                    <li>
                        <figure>
                            <img src="images/truck-img.jpg" alt="" class="rounded">
                        </figure>
                    </li>-->
                </ul>
            </div>

            <div class="mb-8">
                <strong class="block text-base font-bold text-gray-800 mb-2">Type</strong>
                <span class="block text-base text-gray-500">{{profile_truck.truck_type}}: Class {{profile_truck.truck_class}}</span>
            </div>

            <div class="flex mb-5">
                <strong class="flex items-center text-base font-bold text-gray-800">
                    Crane Certification
                    <!--<span class="bg-green-500 text-white w-5 h-5 flex items-center justify-center rounded-sm ml-6">
                        <svg xmlns="http://www.w3.org/2000/svg" class="h-6 w-6" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"></path>
                        </svg>
                    </span>-->
                </strong>
            </div>

            <div class="mb-14">
                <ul>
                    <li class="flex items-center mb-3">
                        <span class="block text-base text-gray-600 w-1/5">
                            Crane cert
                        </span>
                        {% for up in user_pdf.results %}
                            {% if up.page_category == 'truck' and  up.item_category == 'crane_certification' %}
                                <a href="{{up.direct_url}}" class="text-base text-blue-500 hover:underline">View Certificate</a>
                            {% endif %}
                        {% endfor %}  
                    </li>
                    
                </ul>

                <span class="block text-base text-gray-500 my-3">Expiration date: <strong>{{ profile_truck.crane_cert_expiry_date | date: "%d.%m.%Y" }}</strong></span>
                
                <span class="block text-base text-gray-500 mb-4">Capacity under: <strong>{{ profile_truck.capacity }}</strong></span>
            </div>

            {% if truck.air_compressor == 'Yes' %}
            <div class="flex mb-7">
                <strong class="flex items-center text-base font-bold text-gray-800">
                    CompressorAir
                    <!--<span class="bg-green-500 text-white w-5 h-5 flex items-center justify-center rounded-sm ml-6">
                        <svg xmlns="http://www.w3.org/2000/svg" class="h-6 w-6" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"></path>
                        </svg>
                    </span>-->
                </strong>
            </div>
            {%endif%}

            <div class="mb-7">
                <h4 class="mb-4 text-base text-gray-700 font-bold">Tooling</h4>
                <ul class="customCheck flex flex-col gap-3">
                    {% if profile_truck.tooling_basic == '1' %}
                    <li>
                        <input id="tooling1" type="checkbox" checked disabled>
                        <label for="tooling1" class="text-base font-medium text-gray-800">
                        <span><span></span></span>Basic (1/2 drive" w/ power tools)</label>
                    </li>
                    {% endif %}

                    {% if profile_truck.master_set == '1' %}
                    <li>
                        <input id="tooling1" type="checkbox" checked disabled>
                        <label for="tooling1" class="text-base font-medium text-gray-800">
                        <span><span></span></span>Master set of tools</label>
                    </li>
                    {% endif %}

                    {% if profile_truck.tooling_basic_1_drive == '1' %}
                    <li>
                        <input id="tooling1" type="checkbox" checked disabled>
                        <label for="tooling1" class="text-base font-medium text-gray-800">
                        <span><span></span></span>Basic + 1 drive"</label>
                    </li>
                    {% endif %}

                    {% if profile_truck.tooling_speciality_tool == '1' %}
                    <li>
                        <input id="tooling1" type="checkbox" checked disabled>
                        <label for="tooling1" class="text-base font-medium text-gray-800">
                        <span><span></span></span>Speciality tools</label>
                    </li>
                    {% endif %}

                    {% if profile_truck.tooling_generic_diagnostics == '1' %}
                    <li>
                        <input id="tooling1" type="checkbox" checked disabled>
                        <label for="tooling1" class="text-base font-medium text-gray-800">
                        <span><span></span></span>Generic Diagnostic PC</label>
                    </li>
                    {% endif %}
                </ul>
            </div>

            {% if profile_truck.tooling_speciality_tool == '1' %}
            <div>
                <h4 class="mb-4 text-base text-gray-700 font-bold">Speciality Tools</h4>
                <ul class="customCheck flex flex-col gap-3">
                    {% if profile_truck.line_boring == '1' %}
                    <li>
                        <input id="tooling1" type="checkbox" checked disabled>
                        <label for="tooling1" class="text-base font-medium text-gray-800">
                        <span><span></span></span>Line Boring</label>
                    </li>
                    {% endif %}

                    {% if profile_truck.nitrogen_charge_kit == '1' %}
                    <li>
                        <input id="tooling1" type="checkbox" checked disabled>
                        <label for="tooling1" class="text-base font-medium text-gray-800">
                        <span><span></span></span>Nitrogen charge kit</label>
                    </li>
                    {% endif %}

