<div class="py-11 bg-gray-50">
        <div class="container mx-auto px-3.5">
            <div class="flex justify-between flex-col md:flex-row" x-data="">


                {%comment%}
                <div class="w-full mb-6 md:w-1/5 md:mb-0">
                    <ul>
                        <li class="relative">
                            <a href="/do_search?keyword={{keyword}}" class="block leading-10 px-6 text-base font-medium text-indigo-300 border-l-4 border-transparent heavytechs-text-dark-orange heavytechs-border-color-orange bg-white">
                                All results
                            </a>
                        </li>
                        <li class="relative my-2">
                            <a href="/do_search/mechanic?keyword={{keyword}}&search_type=mechanic" class="block leading-10 px-6 text-base font-medium text-indigo-300 border-l-4 border-transparent text-gray-800 font-normal">
                                Mechanics
                            </a>
                        </li>
                        <li class="relative">
                            <a href="/do_search/projects?keyword={{keyword}}&search_type=projects" class="block leading-10 px-6 text-base font-medium text-indigo-300 border-l-4 border-transparent text-gray-800 font-normal">
                                Projects
                            </a>
                        </li>
                    </ul>
                </div>
                {%endcomment%}

                

                <div class="w-full md:w-3/4">
                    <div x-show="openTab === 1">
                        {%if records.results.size > 0 %}
                        {% for s in records.results %}


                        
                        {% if s.fav_for == 'project' %}
                            <div class="bg-white p-5 shadow-sm rounded-md mb-2.5">
                        
                                <div class="flex justify-between flex-col md:flex-row">
                                    <div>
                                        <figure>
                                            
                                            {%if s.photo.photo.url %}
                                            <img src="{{s.photo.photo.url}}" alt="" class="w-36 h-36 rounded-full m-auto mb-5 md:m-0">
                                            {%else%}
                                            <img src="{{ 'images/signup-img3.png' | asset_url }}" alt="" class="w-36 h-36 rounded-full m-auto mb-5 md:m-0">
                                            {%endif%}
                                            
                                        </figure>
                                    </div>

                                    <div class="w-full md:w-4/5">
                                        <div class="flex justify-between items-start">
                                            <div>
                                                <h2 class="text-gray-700 font-bold text-xl mb-2">Contractors name</h2>
                                                <span class="block heavytechs-text-dark-orange text-base mb-3">{{s.profile.name}}</span>
                                            </div>
                                                
                                        </div>
                                        
                                        <p class="text-base text-gray-700 mb-5">{{s.projects.longer_description}}</p>
                                        <ul class="mb-8">
                                            <li class="flex items-left md:items-center mb-1.5 flex-col md:flex-row">
                                                <span class="block text-gray-500 text-base w-28 mb-3 md:mb-0">Location:</span>
                                                <strong class="block text-base text-gray-800 font-medium">{{s.projects.location_of_project}}</strong>
                                            </li>
                                            <li class="flex items-left md:items-center mb-1.5 flex-col md:flex-row">
                                                <span class="block text-gray-500 text-base w-28 mb-3 md:mb-0">Duration:</span>
                                                <strong class="block text-base text-gray-800 font-medium">{{s.projects.duration_type}}</strong>
                                            </li>
                                            
                                        </ul>

                                        <div>
                                            {%comment%}
                                            <a href="#" class="heavytechs-bg-dark-orange text-white heavytechs-border-color-orange border-2 px-6 leading-7 rounded-3xl text-sm inline-block mr-4">Contact Contractor</a>
                                            {%endcomment%}
                                            <a href="/find_project/details?p_id={{s.projects.id}}" class="heavytechs-border-color-orange heavytechs-text-dark-orange text-white heavytechs-border-color-orange border-2 mr-2 px-6 leading-7 rounded-3xl text-base inline-block">View more</a>
                                        </div>
                                        
                                    </div>
                                </div>

                            </div>
                        {% endif %}
                        

                        {% if s.fav_for == 'mechanic' %}
                            <div class="bg-white p-5 shadow-sm rounded-md mb-2.5">
                        
                                <div class="flex justify-between flex-col md:flex-row">
                                    <div>
                                        <figure>
                                            
                                                {%if s.photo.photo.url %}
                                                <img src="{{s.photo.photo.url}}" alt="" class="w-36 h-36 rounded-full m-auto mb-5 md:m-0">
                                                {%else%}
                                                <img src="{{ 'images/signup-img3.png' | asset_url }}" alt="" class="w-36 h-36 rounded-full m-auto mb-5 md:m-0">
                                                {%endif%}
                                            
                                        </figure>
                                    </div>

                                    <div class="w-full md:w-4/5">
                                        <div class="flex justify-between items-start">
                                            <div>
                                                
                                                    <h2 class="text-gray-700 font-bold text-xl mb-2">{{s.profile_bio.name}}</h2>
                                                    <span class="block heavytechs-text-dark-orange text-base mb-3">{{s.profile_bio.position}}</span>
                                                
                                            </div>
                                            
                                            
                                            {%if s.availability == '1'%}
                                            <strong class="text-base text-gray-700 font-normal bg-blue-200 px-2 block leading-7 rounded-md">Available now</strong>    
                                            {%endif%}    
                                            
                                        </div>
                                        
                                        <p class="text-base text-gray-700 mb-5">{{s.profile_bio.short_description}}</p>
                                        <ul class="mb-8">
                                            <li class="flex items-left md:items-center mb-1.5 flex-col md:flex-row">
                                                <span class="block text-gray-500 text-base mr-7 w-28 mb-3 md:mb-0">Company name:</span>
                                                <strong class="block text-base text-gray-800 font-medium">{{s.profile_bio.company_name}}</strong>
                                            </li>
                                            <li class="flex items-left md:items-center mb-1.5 flex-col md:flex-row">
                                                <span class="block text-gray-500 text-base mr-7 w-28 mb-3 md:mb-0">Location:</span>
                                                <strong class="block text-base text-gray-800 font-medium">{{s.profile_bio.location}}, {{s.profile_bio.ready_for_travel}}</strong>
                                            </li>
                                            <li class="flex items-left md:items-center mb-1.5 flex-col md:flex-row">
                                                <span class="block text-gray-500 text-base mr-7 w-28 mb-3 md:mb-0">Experience:</span>
                                                <strong class="block text-base text-gray-800 font-medium">{{s.profile_bio.total_experience}}</strong>
                                            </li>
                                            <li class="flex items-left md:items-center mb-1.5 flex-col md:flex-row">
                                                <span class="block text-gray-500 text-base mr-7 w-28 mb-3 md:mb-0">Drivers License:</span>
                                                <strong class="block text-base text-gray-800 font-medium">{{s.profile_skills.drivers_license_class}}</strong>
                                            </li>
                                        </ul>

                                        <a href="/find_mechanic/details?m_id={{s.fav_id}}" class="heavytechs-bg-dark-orange text-white heavytechs-border-color-orange border-2 px-6 leading-7 rounded-3xl text-sm inline-block">View more</a>
                                    </div>
                                </div>

                            </div>
                        {% endif %}

                        
                        {% endfor %}

                        {%else%}
                        <div class="bg-white shadow-sm rounded-md px-6 py-6 mb-2">No Records Found.</div>
                        {%endif%}

                    
                    </div>

                </div>
            </div>
        </div>
    </div>